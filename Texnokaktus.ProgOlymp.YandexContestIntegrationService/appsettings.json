    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "{Timestamp:HH:mm:ss.fff K} [{Level:u3}] <{SourceContext}> {Message:lj} {NewLine}{Exception}"
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithThreadId",
            "WithExceptionDetails",
            "WithEnvironmentName",
            "WithAssemblyName",
            "WithMachineName"
        ]
    },
